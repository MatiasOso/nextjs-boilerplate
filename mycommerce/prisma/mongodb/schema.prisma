generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL_MONGODB")
}

type Bebidas0 {
  id           String? @map("_id") @db.ObjectId
  cantidad     Int
  descripcion  String
  elaboracion  String
  gramaje      String
  nombre       String
  precio_costo Int
  precio_venta Int
  vencimiento  String
}

type ExperimentoFeatures {
  name  String
  value String
}

type ExperimentoImages {
  description String
  kind        String
  url         String
}

type ExperimentoPhysical {
  color      String
  dimensions ExperimentoPhysicalDimensions
  size       String
  weight     ExperimentoPhysicalWeight
}

type ExperimentoPhysicalDimensions {
  height Float
  length Float
  unit   String
  width  Float
}

type ExperimentoPhysicalWeight {
  unit  String
  value Float
}

type ExperimentoReviews {
  comment   String
  createdAt DateTime @db.Date
  stars     Int
  userId    String   @db.ObjectId
}

model Bebidas {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  /// This field was commented out because of an invalid name. Please provide a valid one that matches [a-zA-Z][a-zA-Z0-9_]*
  // 0 Bebidas0? @map("0")
  cantidad     Int?
  descripcion  String?
  elaboracion  String?
  gramaje      String?
  nombre       String?
  precio_costo Int?
  precio_venta Int?
  vencimiento  String?
}

model Category {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model Chocolates {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  cantidad     Int
  descripcion  String
  elaboracion  String
  gramaje      String
  nombre       String
  precio_costo Int
  precio_venta Int
  url          String
  vencimiento  String
}

model Experimento {
  id          String                @id @default(auto()) @map("_id") @db.ObjectId
  brand       String?
  category    String?
  createdAt   DateTime?             @db.Date
  description String?
  email       String?
  features    ExperimentoFeatures[]
  images      ExperimentoImages[]
  is_active   Boolean?
  name        String
  owner       String?               @db.ObjectId
  parent      String?               @db.ObjectId
  password    String?
  physical    ExperimentoPhysical?
  price       Int?
  profile     String?
  reviews     ExperimentoReviews[]
  stock       Int?
  tags        String[]
  updatedAt   DateTime?             @db.Date
}

model Galletas {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  cantidad     Int
  descripcion  String
  elaboracion  String
  gramaje      String
  nombre       String
  precio_costo Int
  precio_venta Int
  vencimiento  String
}

model Gomitas {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  cantidad     Int
  descripcion  String
  elaboracion  String
  gramaje      String
  nombre       String
  precio_costo Int
  precio_venta Int
  vencimiento  String
}

model Helados {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  cantidad     Int
  descripcion  String
  elaboracion  String
  gramaje      String
  nombre       String
  precio_costo Int
  precio_venta Int
  vencimiento  String
}

model Usuarios {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  name     String
  password String
}
